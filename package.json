{
  "name": "keeper.sh",
  "private": true,
  "license": "AGPL-3.0-only",
  "workspaces": {
    "packages": [
      "packages/*",
      "applications/*"
    ]
  },
  "type": "module",
  "scripts": {
    "clean": "rimraf --glob **/{.turbo,node_modules,.next/}/",
