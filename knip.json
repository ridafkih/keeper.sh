{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": ["**/*.css", "applications/cron/src/jobs/**/*"],
  "ignoreBinaries": [],
  "ignoreDependencies": ["@svgr/webpack"],
  "ignoreWorkspaces": ["packages/typescript-config"],
  "workspaces": {
    "packages/*": {
      "project": "src/**/*.{ts,tsx}"
    },
    "applications/*": {
      "project": "src/**/*.{ts,tsx}"
    },
    "applications/api": {
      "entry": ["src/routes/**/*.ts"],
      "project": "src/**/*.{ts,tsx}"
    },
    "applications/cron": {
      "entry": ["src/jobs/*.ts"],
      "project": "src/**/*.{ts,tsx}"
    }
  }
}
